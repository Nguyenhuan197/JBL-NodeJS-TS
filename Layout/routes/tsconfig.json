{
    "compilerOptions": {
      "target": "ES2015",
      "module": "commonjs",
      "lib": ["DOM", "ES2015"],
      "rootDir": "./Ts",
      "outDir": "",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true
      
    },

    
    "include": [
      "./Ts/**/*.ts"
    ],

    "exclude": [
      "node_modules"
    ],

    
    "watchOptions": {
      "watchFile": "useFsEvents",
      "watchDirectory": "useFsEvents",
      "fallbackPolling": "dynamicPriority"
    }
  }
  