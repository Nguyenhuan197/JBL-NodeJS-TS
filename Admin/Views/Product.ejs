<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin</title>
    <link rel="icon" href="data:image/png;base64," type="image/png">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.0/font/bootstrap-icons.css">
    <link rel="stylesheet" href="CSS/System/Default.css">
    <link rel="stylesheet" href="CSS/Statistical.css">
    <link rel="stylesheet" href="CSS/Product.css">
</head>


<body>
    <section class="Body">
        <section class="Box_Menu">
            <div class="Imae_User"><img src="/Upload/Acount/Acount 1.jpg" alt=""></div>
            <span class="Name_User">Huannd98</span>
            <div class="Icon_Dar_More"><i class="bi bi-brightness-low"></i></div>
            <span class="Name_Theme">Light</span>
            <div class="Icon_Nobell"><i class="bi bi-bell"></i></div>
            <div class="Icon_Email"><i class="bi bi-envelope"></i></div>   
            <div class="Icon_Gitthub"><i class="bi bi-github"></i></div>

            <div class="Box_Count_Nobel">99</div>
            <div class="Box_Count_Email">99</div>
            <div class="Box_Count_Gitthub">99</div>
        </section>

    <!--  -->
  
         <!-- Footer -->
             <%- include('./partials/Menu')  %>
         <!-- Footer -->
 


    <!--  -->

        <section class="Box_Body_Conten">
            <section class="Box_Input" id="Edit_Product" style="display: none;">
                <form id="Upload_Product" enctype="multipart/form-data">
                    <input type="hidden" name="ID" class="ID_Upload">

                    <div class="View_Edit_IMG">
                        <img id="Preview_Image" style="width: 130px;" src="" alt="Xem trước ảnh">
                    </div><br>
                    
                    <label for="IMG_News">Upload File Image</label>
                    <input type="file" name="image" id="IMG_News" required>
                
                    <label>Name</label>
                    <input class="Name_Upload" name="Name" type="text" value="???">
                
                    <label for="Price_Upload">Price</label>
                    <input class="Price_Upload" name="Price" type="text" placeholder="Price" >
                
                    <label for="Category_Upload">Category</label>
                    <select id="Category_Upload" name="Category" required>
                        <option value="Loa To">Loa To</option>
                        <option value="Loa Nhỏ">Loa Nhỏ</option>
                    </select>
                
                    <button type="submit">Cập Nhật Mới</button>
                </form>
            </section>



            <section class="Box_Input" id="Add_Product" style="display: block;">
                <form id="Upload_Product" enctype="multipart/form-data">
                    <input type="hidden" name="ID" class="ID_Upload">

                    <div class="View_Edit_IMG">
                        <img id="Preview_Image" style="width: 130px;" src="" alt="Xem trước ảnh">
                    </div><br>
                    
                    <label for="IMG_News">Upload File Image</label>
                    <input type="file" name="image" id="IMG_ADD" required>
                
                    <label>Name</label>
                    <input class="Name_Add" name="Name" type="text" value="???">
                
                    <label for="Price_Upload">Price</label>
                    <input class="Price_Add" name="Price" type="text" placeholder="Price" >
                
                    <label for="Category_Upload">Category</label>
                    <select id="Category_ADD" name="Category" required>
                        <option value="1">Loa Di Động</option>
                        <option value="2">Loa Tiệc</option>
                        <option value="3">Loa Radio</option>
                    </select>
                
                    <button type="submit">Thêm Mới Hình</button>
                </form>
            </section> 



        

            <section style="display: block;" class="Box_Views ">
                <table>
                    <thead>
                        <tr>
                            <th>STT</th>
                            <th>Name Product</th>
                            <th>Price</th>
                            <th>Category</th>
                            <th>ID Product</th>
                            <th>IMG</th>
                            <th>Trạng Thái</th>
                            <th>Edit</th>
                            <th>Delete</th>
                        </tr>
                    </thead>   

                    <tbody id="productList">
                        <!--  Dữ Liệu được xuất tại đâ -->
                    </tbody>


                </table>
            </section>


        </section>


    </section>
</body>

    <script script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    <script src="Javascript/Product.js"></script>
    <script src="Javascript/Default.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    <script src="/Javascript/Check_Admin.js"></script>

</html>      