    <link rel="stylesheet" href="CSS/System/Default.css">
    <link rel="stylesheet" href="CSS/Statistical.css">
    

    <script src="../Admin/App/Public/Javascript/Admin.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>


</head>
<body>
    <section class="Body">
        <section class="Box_Menu">
            <div class="Imae_User"><img src="/Upload/Acount/Acount 1.jpg" alt=""></div>
            <span class="Name_User">Huannd98</span>
            <div class="Icon_Dar_More"><i class="bi bi-brightness-low"></i></div>
            <span class="Name_Theme">Light</span>
            <div class="Icon_Nobell"><i class="bi bi-bell"></i></div>
            <div class="Icon_Email"><i class="bi bi-envelope"></i></div>   
            <div class="Icon_Gitthub"><i class="bi bi-github"></i></div>
            <div class="Box_Count_Nobel">99</div>
            <div class="Box_Count_Email">99</div>
            <div class="Box_Count_Gitthub">99</div>
        </section>


        
    <!--  -->
         
          <!-- Footer -->
          <%- include('./partials/Menu')  %>
          <!-- Footer -->

    <!--  -->


        


<section class="Box_Body_Conten">
    <h1>Thống Kê Doanh Thu</h1>
    
    <div class="Content-Row">
        <!-- Biểu đồ tròn - Doanh thu trong 1 tuần -->
        <div class="Box">
            <h3 class="Title">Doanh thu tuần</h3>
            <canvas id="revenueChart"></canvas>
        </div>

        <!-- Biểu đồ đường - Đơn hàng bán trong 12 tháng -->
        <div class="Box">
            <h3 class="Title">Đơn hàng bán trong 12 tháng</h3>
            <canvas id="salesChart"></canvas>
        </div>
    </div>
</section>


    <script>
        var ctx1 = document.getElementById('revenueChart').getContext('2d');
        var revenueChart = new Chart(ctx1, {
            type: 'pie',
            data: {
                labels: ['Thứ 2', 'Thứ 3', 'Thứ 4', 'Thứ 5', 'Thứ 6', 'Thứ 7', 'Chủ nhật'],
                datasets: [{
                    label: 'Doanh thu',
                    data: [500, 700, 600, 800, 750, 900, 950], // Dữ liệu doanh thu hàng ngày
                    backgroundColor: ['#FF6384', '#36A2EB', '#FFCE56', '#4BC0C0', '#9966FF', '#FF9F40', '#66FF66'],
                    borderColor: '#fff',
                    borderWidth: 1
                }]
            },
            options: {
                responsive: true,
                plugins: {
                    legend: {
                        position: 'top',
                    },
                    tooltip: {
                        callbacks: {
                            label: function(tooltipItem) {
                                return tooltipItem.label + ': ' + tooltipItem.raw + ' VND'; // Hiển thị đơn vị VND trong tooltip
                            }
                        }
                    }
                }
            }
        });

        // Biểu đồ đường - Đơn hàng bán trong 12 tháng
        var ctx2 = document.getElementById('salesChart').getContext('2d');
        var salesChart = new Chart(ctx2, {
            type: 'line',
            data: {
                labels: ['Tháng 1', 'Tháng 2', 'Tháng 3', 'Tháng 4', 'Tháng 5', 'Tháng 6', 'Tháng 7', 'Tháng 8', 'Tháng 9', 'Tháng 10', 'Tháng 11', 'Tháng 12'],
                datasets: [{
                    label: 'Số lượng đơn hàng',
                    data: [200, 250, 300, 350, 400, 450, 500, 550, 600, 650, 700, 750], // Dữ liệu đơn hàng hàng tháng
                    fill: false,
                    borderColor: '#FF5733',
                    tension: 0.1
                }]
            },
            options: {
                responsive: true,
                plugins: {
                    legend: {
                        position: 'top',
                    },
                    tooltip: {
                        callbacks: {
                            label: function(tooltipItem) {
                                return 'Đơn hàng: ' + tooltipItem.raw; // Hiển thị số đơn hàng trong tooltip
                            }
                        }
                    }
                },
                scales: {
                    y: {
                        beginAtZero: true
                    }
                }
            }
        });


            </script>

        </html>      













