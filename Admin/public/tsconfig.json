{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs",
    "lib": ["DOM", "ES2016"],
    "rootDir": "./Ts",
    "outDir": "./Javascript",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },

  
  "include": [
    "./Ts/**/*.ts"
  ],


  "exclude": [
    "node_modules"
  ],
  "watchOptions": {
    "watchFile": "useFsEvents",
    "watchDirectory": "useFsEvents",
    "fallbackPolling": "dynamicPriority"
  }

}
